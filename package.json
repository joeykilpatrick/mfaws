{
  "name": "mfaws",
  "version": "0.0.3",
  "description": "A cli tool to manage multi-factor IAM User authentication for the AWS CLI.",
  "main": "src/index.js",
  "bin": {
    "mfaws": "bin/mfaws"
  },
  "scripts": {
    "build": "tsc",
    "lint": "./node_modules/.bin/tslint **/*.ts --exclude node_modules/**",
    "test": "./node_modules/.bin/ts-mocha **/*.spec.ts",
    "coverage": "./node_modules/.bin/nyc --reporter=text --reporter=html npm run test"
  },
  "author": "Joey Kilpatrick",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.677.0",
    "chalk": "^4.0.0",
    "meow": "^6.1.1",
    "prompts": "^2.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.5",
    "@types/prompts": "^2.0.8",
    "@types/sinon": "^9.0.4",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "ts-mocha": "^7.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "all": true,
    "include": "src",
    "exclude": [
      "src/config-files/samples",
      "**/*.spec.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeykilpatrick/mfaws.git"
  },
  "keywords": [
    "aws",
    "cli",
    "mfa",
    "multi",
    "factor",
    "iam",
    "cdk",
    "sam",
    "otp",
    "credential",
    "credentials"
  ],
  "bugs": {
    "url": "https://github.com/joeykilpatrick/mfaws/issues"
  },
  "homepage": "https://github.com/joeykilpatrick/mfaws#readme"
}
